# API Configuration
API_URL=https://api.hide-mail.org
API_TIMEOUT=10000

# Email Configuration
EMAIL_DOMAINS=hide-mail.org,private-mail.org
EMAIL_EXPIRATION_TIME=1800
EMAIL_EXTENSION_TIME=1800

# Security Configuration
ENABLE_RATE_LIMITING=true
MAX_REQUESTS_PER_MINUTE=60

# Feature Flags
ENABLE_AUTO_REFRESH=true
ENABLE_NOTIFICATIONS=true

# Production Settings
DEBUG_MODE=false
LOG_LEVEL=error

# CI Settings
DISABLE_ESLINT_PLUGIN=true
CI=false

# Environment
NODE_ENV=production 